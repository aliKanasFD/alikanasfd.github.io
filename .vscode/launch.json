{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Web",
            "type": "chrome",
            "request": "launch",
            "preLaunchTask": "npm: start",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "breakOnLoad": true,
            "sourceMapPathOverrides": {
                "web/*": "${workspaceFolder}/web/*"
            }
        }
    ]
}